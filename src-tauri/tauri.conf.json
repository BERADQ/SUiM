{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "su-ai",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "bundle": {
      "resources": ["./figure.json"],
      "active": true,
      "targets": "all",
      "identifier": "top.ninnana.suai",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": "default-src blob: data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'"
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "su-ai",
        "width": 800,
        "height": 600,
        "minWidth": 800,
        "minHeight": 600,
        "decorations": true,
        "transparent": true
      }
    ]
  }
}
